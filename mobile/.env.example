# Mobile App Environment Variables

# API Configuration
EXPO_PUBLIC_API_BASE_URL=http://192.168.1.100:5000/api/v1
EXPO_PUBLIC_APP_NAME=FisioFlow Mobile

# App Configuration
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_BUILD_NUMBER=1

# Features
EXPO_PUBLIC_ENABLE_BIOMETRIC_AUTH=true
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=true

# Development
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=info

# Push Notifications (Expo)
# EXPO_PUSH_TOKEN=your_expo_push_token_here

# Analytics
# EXPO_PUBLIC_ANALYTICS_KEY=your_analytics_key

# Production (uncomment for production builds)
# EXPO_PUBLIC_API_BASE_URL=https://your-api-domain.com/api/v1
# EXPO_PUBLIC_DEBUG_MODE=false
# EXPO_PUBLIC_LOG_LEVEL=error